<UserControl x:Class="Autobox.Desktop.Controls.TurntableControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Autobox.Desktop.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="900
             ">
    <Grid>
        <StackPanel>
            <Image x:Name="image" Height="100" Source="/Autobox.Desktop;component/Assets/autobox.png" Margin="20, 20, 20, 20"/>
        </StackPanel>
    </Grid>
</UserControl>
